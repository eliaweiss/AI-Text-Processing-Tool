{
  "name": "ai-code-assistant",
  "version": "1.0.0",
  "description": "AI-powered code completion assistant using Granite model",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "dev:text": "vite --open /index-text.html",
    "build": "vite build",
    "build:text": "vite build --outDir dist-text",
    "preview": "vite preview",
    "preview:text": "vite preview --outDir dist-text",
    "serve": "npx http-server -p 8000 -c-1"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.6"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "author": "",
  "license": "MIT"
}
